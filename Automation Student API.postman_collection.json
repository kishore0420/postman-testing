			"item": [
				{
					"name": "Add New Student",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.environment.set(\"Application_LN\", \"TestingWorld\");\r",
									"\r",
									"var jsonData = pm.response.json();\r",
									"var StudID = jsonData.id;\r",
									"pm.environment.set(\"StudentID\", StudID);\r",
									"\r",
									"\r",
									"pm.test(\"Validate Post Status is 201\", function () {\r",
									"    pm.response.to.have.status(201);\r",
									"});\r",
									"\r",
									"pm.test(\"Response time should be less than 500ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(500);\r",
									"});\r",
									"\r",
									"pm.test(\"Validate Status Code String\", function () {\r",
									"\r",
									"    var data = pm.environment.get(\"Result_String\");\r",
									"    pm.response.to.have.status(data);\r",
									"});\r",
									"\r",
									"pm.test(\"Validate Status Header is present\", function () {\r",
									"    pm.response.to.have.header(\"Server\");\r",
									"});\r",
									"\r",
									""
								],
								"type": "text/javascript"
							}
						}
